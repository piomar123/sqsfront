<div class="container">
	<div class="row">
		<div class="col-sm-offset-3 col-sm-6">
			<div class="panel panel-default">
				<div class="panel-heading">
					<h3 class="panel-title">Upload image</h3>
				</div>
				<div class="panel-body">
					<form action="#" method="POST" id="sign-up-form" class="form-horizontal" enctype="multipart/form-data">
						<% /*fields.forEach(function(elem){ %>
							<input type="hidden" name="<%= elem.name  %>" value="<%=elem.value %>"/>
						<%}); */ %>
						<div class="form-group">
							<input type="file" name="file" class="col-sm-12 form-control-file" style="color: #c8c8c8;"/>
						</div>
						<div class="form-group">
							<p class="col-sm-12 text-muted">File will be send to the bucket <b>..<%#=bucket%></b></p>
						</div>
						<div class="form-group">
							<div class="col-sm-offset-2 col-sm-8">
								<input type="submit" class="btn btn-default btn-block" value="Upload"/>
							</div>
						</div>
					</form>
				</div>
			</div>
		</div>
	</div>
</div>
