<div class="container">
	<div class="row">
		<div class="col-sm-12">
			<div class="panel panel-default">
				<div class="panel-heading">
					<h3 class="panel-title">Gallery</h3>
				</div>
				<div class="panel-body">
  				<div class="row">
  					<div class="col-md-12">
              <% thumbs.forEach(function(entry){ %>
                <p><%= entry.name %></p><img src="<%= entry.url %>"/><br/>
              <% }); %>
  					</div>
          </div>
				</div>
			</div>
		</div>
	</div>
</div>
