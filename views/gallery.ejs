<form action="/process" method="post">
	<div class="container">
		<div class="row">
			<div class="col-sm-12">
				<div class="page-header">
					<h1>Gallery</h1>
				</div>
			</div>
			<div class="row">
				<div class="col-md-12">
					<button>Select all</button>
					<input type="submit" value="Process selected" />
				</div>
			</div>
		</div>
	</div>
<div class="container-fluid">
	<div class="row">
    <% thumbs.forEach(function(entry){ %>
			<div class="col-lg-3 col-sm-4 col-xs-6">
				<div class="thumbnail">
					<a href="/image/<%= entry.name %>">
						<img class="img-responsive" src="<%= entry.thumb %>" />
					</a>
					<div class="caption">
						<p style="word-wrap: break-word;"><input type="checkbox" name="images[]" value="<%= entry.name %>" />
						<%= entry.name %></p>
					</div>
				</div>
			</div>
    <% }); %>
		</div>
	</div>
</div>
</form>
